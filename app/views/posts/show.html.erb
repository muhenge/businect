<%= @post.content %>
<%= link_to @post_user.username.capitalize, user_path(@user) %>
<%= link_to like_post_path(@post), method: :put do %>
    <span class="medium material-icons">thumb_up</span>
    <%= @post.get_upvotes.size %>
<% end %>

 <%= image_tag(current_user.avatar,:class => "user-img responsive-img", alt: "user image") if current_user.avatar.attached?%>
